<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="true" enabled="true" name="extract-org-and-app-ids">
    <DisplayName>Extract org and app ids</DisplayName>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>  
    <URIPath>
      <Pattern>/{orgid}/{appid}/**</Pattern>
    </URIPath>
    <Source clearPayload="false">request</Source>
</ExtractVariables>