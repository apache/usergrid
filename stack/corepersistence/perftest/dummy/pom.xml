<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.apache.usergrid.perftest</groupId>
    <artifactId>perftest-parent</artifactId>
    <version>1.0-SNAPSHOT</version>
  </parent>

  <artifactId>dummy</artifactId>
  <packaging>jar</packaging>
  <name>Dummy Test</name>

  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.usergrid.perftest</groupId>
        <artifactId>perftest-maven-plugin</artifactId>
        <version>1.3-SNAPSHOT</version>
        <configuration>
          <accessKey>${accessKey}</accessKey>
          <secretKey>${secretKey}</secretKey>
          <bucketName>perftest-uploadplugin-test</bucketName>
          <gitConfigDirectory>${project.parent.basedir}/.git</gitConfigDirectory>
          <perftestFormation>perftestform</perftestFormation>
          <managerAppUsername>managerusername</managerAppUsername>
          <managerAppPassword>managerpass</managerAppPassword>
          <testModuleFQCN>testmodfqcn</testModuleFQCN>
          <failIfCommitNecessary>false</failIfCommitNecessary>
        </configuration>
        <!-- We run the execution goal manually after build, since this project is under the same root pom
            as the plugin's. You can uncomment the following execution block to run plugin goal automatically -->
        <!-- <executions>
          <execution>
            <id>UploadWarToS3</id>
            <phase>test</phase>
            <goals>
              <goal>perftest-war</goal>
            </goals>
          </execution>
        </executions>-->
      </plugin>
    </plugins>
  </build>
</project>
